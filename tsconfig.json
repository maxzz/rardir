{
    "compilerOptions": {
        "target": "es2020",
        "module": "esnext",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "moduleResolution": "bundler",
        
        "noEmit": true,
        "strict": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
    },
    "include": [
        "src/*"
    ],
}
